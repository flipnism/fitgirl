{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
           
        }
    ],
    "functions":{
        "index.js":{
            "includeFiles": ["views/*","public/**","gamelist.json"]
        }
    },
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ]
  }